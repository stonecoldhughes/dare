<config>

    <wrap_above_func> trace_cpu_start </wrap_above_func>
    
    <wrap_below_func> trace_cpu_stop </wrap_below_func>

    <wrap_above_args>
    </wrap_above_args>

    <wrap_below_args>
        <arg configurable = 'True' string = 'False'> color_list </arg>
    </wrap_below_args>

    <color_list>
        <pair> <key> default </key> <value> 0xFFFF </value> </pair>
        <pair> <key> core_dgemm </key> <value> 0xCCCC </value> </pair>
        <pair> <key> core_dsyrk </key> <value> 0xAAAA </value> </pair>
    </color_list>
    
</config>
